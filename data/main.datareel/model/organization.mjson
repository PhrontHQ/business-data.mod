{
    "root": {
        "prototype": "montage/core/meta/module-object-descriptor",
        "values": {
            "name": "Organization",
            "propertyDescriptors": [
                {"@": "name"},
                {"@": "type"},
                {"@": "email"},
                {"@": "phone"},
                {"@": "addresses"},
                {"@": "parent"},
                {"@": "suborganizations"},
                {"@": "tags"},
                {"@": "mainContact"},
                {"@": "employmentPositions"},
                {"@": "b2cCustomerRelationships"},
                {"@": "b2bCustomerRelationships"},
                {"@": "supplierRelationships"},
                {"@": "services"}
            ],
            "objectDescriptorModule": {
                "%": "data/main.datareel/model/organization.mjson"
            },
            "exportName": "Organization",
            "module": {
                "%": "data/main.datareel/model/organization"
            },
            "object":{"@": "organization"},
            "parent":{"@": "PartyDescriptor"},
            "userInterfaceDescriptorModules": {
                "*": {
                    "%": "./organization-ui-descriptor.mjson"
                }
            }

        }
    },
    "organization": {
        "object": "data/main.datareel/model/organization"
    },

    "PartyDescriptor": {
        "object": "data/main.datareel/model/party.mjson"
    },

    "name": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "name",
            "valueType": "string",
            "isSearchable": true
        }
    },
    "type": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "type",
            "valueType": "string"
        }
    },
    "email": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "email",
            "valueType": "string"
        }
    },
    "phone": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "phone",
            "valueType": "string"
        }
    },
    "addresses": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "addresses",
            "valueType": "array",
            "cardinality": -1,
            "valueDescriptor": {"@": "addresses_descriptor"},
            "isSearchable": true
       }
    },

    "parent": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "parent",
            "valueType": "object",
            "cardinality": 1,
            "valueDescriptor": {"@": "root"}
       }
    },

    "suborganizations": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "suborganizations",
            "valueType": "array",
            "cardinality": -1,
            "valueDescriptor": {"@": "root"}
       }
    },

    "addresses_descriptor": {
        "object": "data/main.datareel/model/messaging-channel/postal-address.mjson"
    },


    "tags": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "tags",
            "valueType": "string",
            "cardinality": -1
       }
    },

    "mainContact": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "mainContact",
            "valueType": "object",
            "cardinality": 1,
            "valueDescriptor": {"@": "personDescriptor"}
       }
    },
    "personDescriptor": {
        "object": "data/main.datareel/model/person.mjson"
    },

    "employmentPositions": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "employmentPositions",
            "valueType": "object",
            "cardinality": -1,
            "valueDescriptor": {"@": "employmentPositionDescriptor"}
       }
    },
    "employmentPositionDescriptor": {
        "object": "data/main.datareel/model/employment-position.mjson"
    },
    "employees": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "employees",
            "valueType": "object",
            "cardinality": -1,
            "definition": "employmentPositions.map{staffingHistory.employee}"
       }
    },
    "b2cCustomerRelationships": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "b2cCustomerRelationships",
            "valueType": "array",
            "cardinality": -1,
            "valueDescriptor": {"@": "B2CCustomerSupplierRelationship"}
       }
    },
    "B2CCustomerSupplierRelationship": {
        "object": "data/main.datareel/model/b-2-c-customer-supplier-relationship"
    },

    "b2bCustomerRelationships": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "b2bCustomerRelationships",
            "valueType": "array",
            "cardinality": -1,
            "valueDescriptor": {"@": "B2BCustomerSupplierRelationship"}
       }
    },
    "B2BCustomerSupplierRelationship": {
        "object": "data/main.datareel/model/b-2-b-customer-supplier-relationship"
    },

    "supplierRelationships": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "supplierRelationships",
            "valueType": "array",
            "cardinality": -1,
            "valueDescriptor": {"@": "root"}
       }
    },

    "services": {
        "prototype": "montage/core/meta/property-descriptor",
        "values": {
            "name": "services",
            "valueType": "array",
            "cardinality": -1,
            "inversePropertyName": "vendors",
            "valueDescriptor": {"@": "servicesDescriptor"}
       }
    },

    "servicesDescriptor": {
        "object": "data/main.datareel/model/service.mjson"
    }

}
